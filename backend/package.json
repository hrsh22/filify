{
  "name": "filecoin-ens-backend",
  "version": "1.0.0",
  "description": "Backend API for Filecoin Pin + ENS deployment platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:init": "tsx scripts/init-db.ts",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate": "drizzle-kit up:sqlite --schema=./src/db/schema.ts --driver=better-sqlite --url=./data/dev.db",
    "db:push": "mkdir -p data && drizzle-kit push:sqlite --schema=./src/db/schema.ts --url=./data/dev.db --driver=better-sqlite",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "archiver": "^7.0.1",
    "connect-sqlite3": "^0.9.13",
    "content-hash": "^2.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "multiformats": "^13.3.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/connect-sqlite3": "^0.9.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.5",
    "@types/passport": "^1.0.16",
    "@types/passport-github2": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "better-sqlite3": "^9.2.2",
    "drizzle-kit": "^0.20.9",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
