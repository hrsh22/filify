{
  "name": "filecoin-ens-backend",
  "version": "1.0.0",
  "description": "Backend API for Filecoin Pin + ENS deployment platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:init": "tsx scripts/init-db.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:prod": "node scripts/apply-migrations.js",
    "db:apply": "tsx scripts/apply-migrations.ts",
    "db:push": "mkdir -p data && drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@filoz/synapse-sdk": "^0.36.1",
    "@ipld/car": "^5.4.2",
    "@ipld/dag-pb": "^4.1.5",
    "@libsql/client": "^0.15.15",
    "@octokit/auth-app": "^8.1.2",
    "@octokit/rest": "^20.0.2",
    "archiver": "^7.0.1",
    "blockstore-core": "^6.1.1",
    "connect-sqlite3": "^0.9.13",
    "content-hash": "^2.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.31.0",
    "ethers": "^6.9.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "filecoin-pin": "^0.14.0",
    "helmet": "^7.1.0",
    "interface-blockstore": "^6.0.1",
    "ipfs-unixfs-importer": "^15.4.0",
    "multiformats": "^13.3.1",
    "pino": "^10.1.0",
    "siwe": "^3.0.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/connect-sqlite3": "^0.9.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "better-sqlite3": "^9.2.2",
    "drizzle-kit": "^0.24.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
